@model List<Motos>

@{
    ViewBag.Title = "ViewMotos";
}

<h2>View Motos</h2>

<table border="1">
    <tbody>
        Last Moto
        @foreach (var item in Model)
        {
            <tr>
                <td> @item.SellerName</td>
                <td> @item.Brand  @item.Model </td>
                <td> @item.Price </td>
                <td> @item.Fuel</td>
                <td>@item.Gear</td>
                <td>@item.Color</td>
            </tr>
        }

    </tbody>
</table>

<style>
    td{
       text-align:center
    }
</style>